{
  "name": "bash-ai-roided",
  "version": "1.0.15",
  "main": "bin/bash-ai-roided",
  "author": "screamingpanic <[axelmarora@proton.me]>",
  "bin": {
    "??": "bin/bash-ai-roided"
  },
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "ts-node-dev --respawn src/app.ts",
    "clean": "tsc --build --clean",
    "build": "tsc",
    "buildw": "nodemon --ext ts -w src --legacy-watch --exec  'tsc'",
    "preinstall": "ls -l | grep 'node_modules' && echo 'preinstall'",
    "postinstall": "npm run clean && npm run build && echo 'Installed.'",
    "prepare": "echo 'Prepared.'",
    "prepublishOnly": "npm run clean && npm run build && echo 'Prepublish.'",
    "preversion": "npm run clean && npm run build",
    "version": "npm run clean && npm run build",
    "postversion": "git push && git push --tags"
  },
  "exports": {
    ".": {
      "import": "./dist/app.js",
      "require": "./dist/app.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@types/node": "^18.15.11",
    "commander": "^10.0.1",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prompt-sync": "^4.2.0",
    "ts-node-dev": "^2.0.0"
  },
  "description": "Bash AI Roided is a fork of Bash AI. It improves on the original. Bash AI is a command-line tool that uses the power of OpenAI's GPT-3 to provide intelligent suggestions and automation for your bash commands.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GrimReaper420/BashAIRoided.git"
  },
  "keywords": [
    "gpt-3",
    "ai",
    "open-ai"
  ],
  "bugs": {
    "url": "https://github.com/GrimReaper420/BashAIRoided/issues"
  },
  "homepage": "https://github.com/GrimReaper420/BashAIRoided#readme",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
